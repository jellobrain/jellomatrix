function save_imagepng($canvas,$file){
  // Store output.
  ob_start();
  // Output to buffer.
  imagePNG($canvas);
  // Write buffer to file.
  file_put_contents($file, ob_get_contents(), FILE_BINARY);
  // Clear and turn off buffer.
  ob_end_clean();
}

