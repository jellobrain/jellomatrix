              }
            }
          }
        }
      }
    }
  }
  if (isset($b_scale_sum_ratios['rtl']) && isset($b_scale_sum_ratios['ltr'])) {
    $scaled .= '<br><span class="salmon-text">BACKWARD SLASH DIAGONAL SCALE [\] (' . $b_scale_sum_ratios['rtl'] . '/' . $b_scale_sum_ratios['ltr'] . '): ';
